{
  "name": "mokia",
  "version": "0.4.0",
  "description": "A simple mock server.",
  "main": "libs/index.js",
  "types": "types/index.d.ts",
  "bin": "bin/cli.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "clean": "rimraf ./libs/**",
    "build": "npm run clean && tsc",
    "test": "cross-env DEBUG=mokia:* mocha",
    "test:watch": "cross-env DEBUG=mokia:* nyc mocha -w",
    "test:nyc": "cross-env DEBUG=mokia:* nyc mocha",
    "demo:ts": "cross-env NODE_ENV=development node ./bin/cli ./example/index.ts --watch",
    "demo:js": "cross-env NODE_ENV=development node ./bin/cli ./example/index.js --watch"
  },
  "files": [
    "/bin",
    "/libs",
    "/types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varHarrie/mokia.git"
  },
  "keywords": [
    "mock",
    "server"
  ],
  "author": "varharrie <varharrie@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/varHarrie/mokia/issues"
  },
  "homepage": "https://github.com/varHarrie/mokia#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/debug": "4.1.4",
    "@types/express": "^4.16.1",
    "@types/meow": "^5.0.0",
    "@types/node": "^11.13.11",
    "body-parser": "^1.19.0",
    "canvas": "^2.5.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "dayjs": "^1.8.14",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "meow": "^5.0.0",
    "ora": "^3.4.0",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  }
}
